     name : Evénement_JaugeRemplissage
     procedure_id : 1751915300331672021
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //Evénement_JaugeRemplissage (<nAjout> [, <nTotal>])
      //
      // Paramètres :
      //	nAjout : <indiquez ici le rôle de nAjout>
      //	nTotal : <indiquez ici le rôle de nTotal>
      // Valeur de retour :
      // 	Aucune
      //
      Procédure Evénement_JaugeRemplissage(local nAjout, LOCAL nTotal = JAUGE_REMPLISSAGE..BorneMax)
      
      	nPourcentageUtilisé est un entier
      	nTailleOccupée		est un entier sur 8
      
      	// Modifie la borne maximale
      	si nTotal <> JAUGE_REMPLISSAGE..BorneMax ALORS
      		JAUGE_REMPLISSAGE..BorneMax = nTotal
      	FIN
      	
      	// Complète la jauge
      	si nAjout = 0 ALORS
      		JAUGE_REMPLISSAGE = 0
      	sinon
      		JAUGE_REMPLISSAGE += nAjout		
      	FIN	
      	
      	// Récupère l'espace occupé (réel)
      	nTailleOccupée = val(GraveurInfoCompilation(compilationBlocsUtilisés)) * val(GraveurInfoCompilation(compilationTailleBloc))
      	
      	// Affiche la taille utilisée (sans les entêtes)
      	LIB_TAILLEUTILISEE = fTailleFormat(JAUGE_REMPLISSAGE)
      	
      	// Calcule le pourcentage d'utilisation du disque
      	nPourcentageUtilisé = (nTailleOccupée * 100 / nTotal)
      	
      	nCouleur  est un entier = RVB(134, 227, 134)
      	
      	// Décrémente la couleur en fonction du pourcentage
      	nCouleur = TSL(CouleurTeinte(nCouleur) * max(0, (100 - (nPourcentageUtilisé + modulo(nPourcentageUtilisé, 10)))) / 100, 50, CouleurSaturation(nCouleur) + modulo(nCouleur, 10))
      	
      	// Modifie la couleur du libellé (vert : espace ok, et plus cela tend vers le rouge, plus cela indique que l'espace va bientôt manquer)
      	LIB_TAILLEUTILISEE..couleur = nCouleur
      	
      	fenrepeint()
     type : 458752
