        code : |1+
         
         si gbMédiaPrésent = faux alors
         	
         	// Ouvre le tiroir du graveur
         	GraveurEjecte(vrai)
         	
         	// Attend un média
         	mafenetre..plan = 3
         	retour
         	
         FIN
         
         // Pas de fichier à graver
         si gsListeFichiers = "" ALORS
         		
         	Avertissement("Aucun fichier à graver")
         	retour
         FIN
         
         BTN_GRAVURE..Etat = Grisé
         Multitâche()
         
         // Fin de la détection
         fDétecteStockageAmovible("")
         
         IMG_ANIMDVD..visible 	= vrai
         IMG_ANIMDVD..Animation 	= Vrai
         BTN_EJECTER_MEDIA..etat = grise
         BTN_NOM_MEDIA..etat 	= grise
         BTN_LISTE_FICHIERS..etat= grise
         BTN_AJOUT_FICHIER..etat = grise
         IMG_TYPE_MEDIA..etat 	= inactif
         
         nOptionsGravure est un entier
         si EnModeTest() ALORS
         	
         	// En mode test : simulation de la gravure
         	nOptionsGravure = graveurSimule
         FIN
         
         // Ejection automatique en fin de gravure
         nOptionsGravure += graveurEjecteAuto
         
         // Lance la gravure (à la fin de la gravure, on éjecte le média
         si GraveurEnregistre(nOptionsGravure, Evénement_GravureEncours) ALORS
         	
         	// Vide la liste des fichiers
         	gsListeFichiers			= ""	
         FIN
