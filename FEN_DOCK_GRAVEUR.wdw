     code : |1-
      
      GR_AIDE..visible = faux
      
      gbTiroirOuvert 		est un booléen = faux		// Tiroir du graveur (par défaut : fermé)
      gbGraveurPrésent 	est un booléen = faux		// Etat du graveur
      gbMédiaPrésent		est un booléen = faux		// Média présent dans le lecteur
      gnTailleTotale		est un entier sur 8			// Taille totale du média
      gnTailleUtilisée	est un entier sur 8			// Taille totale utilisée
      gnTailleCompilation	est un entier sur 8			// Taille totale de la compilation
      gnTailleLeads		est un entier				// Taille des blocs d'entête de compilation
      gnTypeMedia			est un entier				// Type du média inséré
      gsListeFichiers		est une chaine				// Liste des fichiers de la compilation
      gsGraveurChemin		est une chaine 				// Chemin du graveur
      TH_Ajouté			est un booléen				// Variable utilisée par les fonctions d'ajout de fichier/répertoire
      TH_AjoutEnCours		est un booléen				// Variable utilisée par les fonctions d'ajout de fichier/répertoire
      
      
      // Gère l'événement d'insertion d'un CDROM
      evenement(Evénement_InsertionCD, "*.*", "INSERTION_CDROM")
      
      // Gère l'événement de fin de gravure
      evenement(Evénement_FinGravure, "*.*", "FIN_GRAVURE")
      
      // En mode test, la gravure est simulée
      si EnModeTest() ALORS
      	mafenetre..titre += " - Mode simulation"
      	LIB_SIMULATION..visible = vrai
      sinon
      	LIB_SIMULATION..visible = faux
      FIN
