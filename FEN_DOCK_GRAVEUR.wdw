   -
     name : RécupèreTypeMédia
     internal_properties : CAAAAAgAAACmXmVWAOY6WGwBmlBIUBMhN3JObuzBFj9SNYTxlri//QP3hypPqiG2u3Gvo5bFkUHGWS7veOWd1Qh071Bg6fR/vHqxz3FWPH9fd3AvQjd2bHiUv37MWA/5xERTaYuSylZZ38MwD+vnaW4YT46IeMZssryXq09BaDoebCpSuuYtang9biLBMkBdxKPKjC0tcsIOx+xXsOI1
     procedure_id : 1751850540801784304
     type_code : 14
     code : |1+
      // Résumé : Affiche le type de média inséré dans le graveur
      // Syntaxe :
      //RécupèreTypeMédia ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //// Traitement automatique des erreurs :   exécuter le bloc de code CAS ERREUR
      //
      
      Procédure RécupèreTypeMédia()
      
      	// Le graveur doit être inactif
      	si RécupèreEtatGraveur() <> GraveurInactif alors retour
      		
      	// Conserve le type de média
      	gnTypeMedia = GraveurInfoMédia(médiaFormat)
      		
      	// En fonction du type de média inséré
      	selon gnTypeMedia
      		
      		// Disque Blu-Ray
      		cas médiaFormat_BD_R, médiaFormat_BD_Re, médiaFormat_BD_ROM
      			
      			// Icône 'Blu-ray'
      			IMG_TYPE_MEDIA = "\images\type-bluray.png"
      			IMG_TYPE_MEDIA..CurseurSouris = curMainSystème
      			IMG_TYPE_MEDIA..bulle = "Disque Blu-Ray"
      
      			// Modifie l'image de fond de la fenêtre
      			MaFenêtre..ImageFond = "\images\graveur-pret.png"
      			gbMédiaPrésent = vrai
      			BTN_GRAVURE..Etat = Actif
      			
      			GraveurNomVolume("BR-" + datedujour)
      		
      		// Disque DVD
      		cas médiaFormat_DVD_R_HD, médiaFormat_DVD_RAM, médiaFormat_DVD_RAM_HD, médiaFormat_DVD_ROM, médiaFormat_DVD_ROM_HD, médiaFormat_DVDmoinsR, ...
      		 	médiaFormat_DVDmoinsR_DL, médiaFormat_DVDmoinsRW, médiaFormat_DVDplusR, médiaFormat_DVDplusR_DL, médiaFormat_DVDplusRW, médiaFormat_DVDplusRW_DL
      		 	
      			// Icône 'DVD'
      			IMG_TYPE_MEDIA = "\images\type-dvd.png"
      			IMG_TYPE_MEDIA..CurseurSouris = curMainSystème
      			IMG_TYPE_MEDIA..Bulle = "Disque DVD"
      
      			// Modifie l'image de fond de la fenêtre
      			MaFenêtre..ImageFond = "\images\graveur-pret.png"		
      			gbMédiaPrésent = Vrai	
      			BTN_GRAVURE..etat = Actif
      			
      			GraveurNomVolume("DVD-" + DateDuJour)
      
      		 // Disque CD
      		cas médiaFormat_r, médiaFormat_Rw
      			
      			// Vérifie si c'est un CD Audio
      			si GraveurInfoMédia(médiaType) = médiaType_CDDA_CDROM ALORS
      
      				// Icône 'CD-a'
      				IMG_TYPE_MEDIA = "\images\type-cda.png"				
      			sinon
      				
      				// Icône 'CD'
      				IMG_TYPE_MEDIA = "\images\type-cd.png"	
      			FIN
      			
      			// Modifie le curseur
      			IMG_TYPE_MEDIA..CurseurSouris = curMainSystème
      			IMG_TYPE_MEDIA..Bulle = "Compact-Disc"
      			
      			// Modifie l'image de fond de la fenêtre
      			MaFenêtre..ImageFond = "\images\graveur-pret.png"
      			gbMédiaPrésent = Vrai
      			BTN_GRAVURE..Etat = actif
      			
      			GraveurNomVolume("CD-" + DateDuJour)
      						
      		// Disque de type inconnu ou inutilisable
      		AUTRE CAS
      			
      			// Aucune icône spécifique
      			IMG_TYPE_MEDIA = ""
      			IMG_TYPE_MEDIA..CurseurSouris = curFlèche
      			IMG_TYPE_MEDIA..bulle = ""
      
      			// Modifie l'image de fond de la fenêtre
      			MaFenêtre..ImageFond = "\images\graveur-vide.png"		
      			gbMédiaPrésent = faux
      			BTN_GRAVURE..Etat = Grisé
      			
      			// Ouvre le tiroir du graveur
      			GraveurEjecte(vrai)
      			
      			mafenetre..plan = 3
      	FIN
     type : 458752
   -
     name : RécupèreInfoMedia
     internal_properties : CAAAAAgAAACCwx1VO57i7fb297D/Nfs2OIUqpfrn+ZA1D9rEGFXAT/43LTFHQ2mYuerzoSjczonkXzFQqjyPcu2d4py58E9CB531WkrIvVwlpmSH/RB1eqGe/cRHSt2+nP4LnL1olVzB/qwfhEGuxTrOpj1Z
     procedure_id : 1751857429929357958
     type_code : 14
     code : |1-
      // Résumé : Récupère des infos sur le média inséré
      // Syntaxe :
      //RécupèreInfoMedia ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      Procédure RécupèreInfoMedia()
      
      	nEtatGraveur est un entier
      	
      	// Récupère l'état du graveur
      	nEtatGraveur = RécupèreEtatGraveur()
      
      	// Le graveur ne doit pas être en cours d'utilisation
      	si nEtatGraveur <> graveurInactif alors
      				
      		// L'éjection n'est possible
      		BTN_EJECTER_MEDIA..Etat = grise
      		BTN_LISTE_FICHIERS..Etat = grise
      		BTN_NOM_MEDIA..Etat = grise
      		BTN_AJOUT_FICHIER..Etat = grise
      		
      		// Pas de remplissage
      		Evénement_JaugeRemplissage(0)
      		
      		// Aucune icône spécifique
      		IMG_TYPE_MEDIA = ""
      		IMG_TYPE_MEDIA..CurseurSouris = curFlèche
      		
      		gbMédiaPrésent = faux	
      	sinon
      			
      		////
      		Sablier(Vrai)
      		IMG_SABLIER..Animation = Vrai
      		
      		// Affiche le type (CD, DVD ou Blu-Ray)
      		RécupèreTypeMédia()
      		
      		nTailleMaximale est un entier sur 8
      		nTailleUtilisée	est un entier sur 8
      		
      		// Récupère la taille maximale (en blocs) du média inséré
      		nTailleMaximale = GraveurInfoMédia(médiaTailleTotale)
      		
      		// Récupère la taille utilisée (en blocs) du média inséré
      		nTailleUtilisée = GraveurInfoMédia(médiaTailleUtilisée)
      		nTailleUtilisée = max(0, nTailleUtilisée)
      		
      		// Cas des CD-a
      		SI nTailleMaximale <= 0 ALORS
      			
      			// On adjuge une taille par défaut pour les CD-a
      			// 2 048 octets par blocs
      			//	  75 blocs par seconde
      			//    60 secondes par minute
      			//    74 minutes par CD
      			// -------------------------
      			// 681 984 000 octets (soit ~650 Mo)
      			nTailleMaximale = 75 * 60 * 74
      		FIN
      		
      		// Récupère la taille totale en octets
      		nTailleMaximale = 2048 * nTailleMaximale
      		
      		// Récupère la taille utilisée en octets
      		nTailleUtilisée = 2048 * nTailleUtilisée
      		
      		// Récupère la taille des entêtes
      		gnTailleLeads   = val(GraveurInfoCompilation(compilationBlocsUtilisés)) * val(GraveurInfoCompilation(compilationTailleBloc))
      		
      		// Conserve les tailles
      		gnTailleTotale 		= nTailleMaximale
      		gnTailleUtilisée	= nTailleUtilisée
      		
      		// Remplissage actuel		
      		Evénement_JaugeRemplissage(nTailleUtilisée, nTailleMaximale)
      		
      		// Affiche la taille max
      		LIB_TAILLEMAX = fTailleFormat(gnTailleTotale)
      		
      		// L'éjection est possible
      		BTN_EJECTER_MEDIA..Etat = Actif
      		BTN_LISTE_FICHIERS..Etat = Actif
      		BTN_NOM_MEDIA..Etat = Actif
      		BTN_AJOUT_FICHIER..Etat = Actif
      		
      		IMG_SABLIER..Animation = faux
      		Sablier(Faux)
      		////
      	FIN
      	
     type : 458752
   -
     name : Evénement_DragAndDropExplorateur
     procedure_id : 1751864611114840667
     type_code : 14
     code : |1-
      // Résumé : Gère l'événement de drag 'n drop de fichiers depuis l'explorateur
      // Syntaxe :
      //Evénement_DragAndDropExplorateur ( [<bDepuisExplorateur> est booléen [, <sListeFichiers> est chaîne]])
      //
      // Paramètres :
      //	bDepuisExplorateur (booléen - valeur par défaut=1) :<indiquez ici le rôle de bDepuisExplorateur>
      //	sListeFichiers (chaîne ANSI - valeur par défaut="") :<indiquez ici le rôle de sListeFichiers>
      // Valeur de retour :
      // 	Aucune
      //
      
      Procédure Evénement_DragAndDropExplorateur(local bDepuisExplorateur est un booléen = vrai, local sListeFichiers est une chaine = "")
      
      	nNbFichiersDroppés 		est un entier
      	sFichier				est une chaine
      	nTailleFichier			est un entier sur 8
      	sListeErreurs			est une chaine
      	
      	// Pas de média, ni de graveur, on sort
      	SI gbMédiaPrésent = Faux _ou_ gbGraveurPrésent = faux ALORS
      		retour
      	FIN
      	
      	sablier(vrai)
      	
      	LIB_TITRE = "Ajout de données..."
      		
      	IMG_SABLIER..Animation = Vrai
      	
      	// Récupère le nombre de fichiers
      	si bDepuisExplorateur alors
      		nNbFichiersDroppés = ExplorerRécupère(_eve.wparam)
      	sinon
      		nNbFichiersDroppés = ChaîneOccurrence(sListeFichiers, rc) + 1
      	fin
      	
      	// Récupère les fichiers
      	pour i = 1 _a_ nNbFichiersDroppés
      		
      		// Récupère le chemin du fichier
      		SI bDepuisExplorateur ALORS
      			sFichier = ExplorerRécupère(_eve.wparam, i)
      		sinon
      			sFichier = Extraitchaine(sListeFichiers, i, rc)
      		fin
      		
      		// Vérifie s'il s'agit d'un répertoire ou d'un fichier
      		si fRépertoireExiste(sFichier + ["\"]) = Vrai ALORS
      			
      			// Récupère la taille du répertoire
      			nTailleFichier = frepTaille(sFichier, frRécursif)
      			
      			// Vérifie si l'ajout du répertoire ne va pas provoquer un dépassement de la taille du média
      			si VérifieTailleCompilation(nTailleFichier) = vrai alors
      			
      				LIB_INFO = sFichier
      			
      				// C'est un répertoire, on doit prendre tous les fichiers qu'il contient
      				TH_AjoutEnCours = Vrai
      				TH_GraveurAjouteRépertoire(sFichier, graveurAjoutRécursif + graveurAjoutInterruptible) 
      				
      				// Attend la fin de l'ajout
      				TANTQUE TH_AjoutEnCours
      					ThreadPause(50)
      				FIN
      				
      				SI TH_Ajouté alors
      					
      					// Complète la liste des fichiers
      					gsListeFichiers += [RC] + "REP\" + extraitchaine(sFichier, rangDernier, "\") + tab + sFichier
      					
      					// Complète la jauge de remplissage
      					Evénement_JaugeRemplissage(nTailleFichier)
      					
      					// Complète la taille de la compilation			
      					gnTailleCompilation = GraveurInfoCompilation(compilationBlocsUtilisés)	
      					
      					// Ajoute les autres fichiers/répertoires
      					continuer
      				fin
      				
      			fin
      			
      			// Si la vérification a échoué ou si l'ajout du répertoire a échoué
      			// On complète l'historique des erreurs	
      			sListeErreurs += [rc] + chaineconstruit("Impossible d'ajouter le répertoire <[%sFichier%]> : La taille de ce répertoire (%1) est trop importante pour qu'il puisse être ajouté (espace disponible : %2)", fTailleFormat(nTailleFichier), fTailleFormat(gnTailleTotale - (gnTailleUtilisée + gnTailleCompilation)))
      			
      		sinon
      			// Récupère la taille du fichier
      			nTailleFichier = fTaille(sFichier)
      			
      			// Vérifie si l'ajout du fichier ne va pas provoquer un dépassement de la taille du média
      			SI VérifieTailleCompilation(nTailleFichier) = Vrai ALORS
      				
      				LIB_INFO = sFichier
      			
      				// C'est un fichier, on l'ajoute à la liste
      				TH_AjoutEnCours = Vrai
      				TH_GraveurAjouteFichier(sFichier)
      				
      				// Attend la fin de l'ajout
      				tantque TH_AjoutEnCours
      					ThreadPause(50)
      				fin
      				
      				// Si le fichier a bien été ajouté
      				si TH_Ajouté alors
      					
      					// Complète la liste des fichiers
      					gsListeFichiers += [RC] + "FIC\" + fExtraitChemin(sFichier, fFichier+fExtension)
      					
      					// Complète la jauge de remplissage
      					Evénement_JaugeRemplissage(nTailleFichier)
      				
      					// Complète la taille de la compilation			
      					gnTailleCompilation += nTailleFichier
      					
      					// Ajoute les autres fichiers/répertoires
      					continuer
      				fin
      				
      			fin
      			
      			// Si la vérification a échoué ou si l'ajout du répertoire a échoué
      			// On complète l'historique des erreurs	
      			sListeErreurs += [RC] + ChaîneConstruit("Impossible d'ajouter le fichier <[%sFichier%]> : La taille de ce fichier (%1) est trop importante pour qu'il puisse être ajouté (espace disponible : %2)", fTailleFormat(nTailleFichier), fTailleFormat(gnTailleTotale - (gnTailleUtilisée + gnTailleCompilation)))
      			
      		FIN
      	FIN
      	
      	IMG_SABLIER..Animation = Faux
      	IMG_SABLIER = IMG_SABLIER	
      	Sablier(faux)
      
      	LIB_TITRE = "Prêt"
      	lib_info  = ""
      
      	// Il y a eu des erreurs
      	si sListeErreurs <> "" alors
      		
      		// Affiche les erreurs
      		erreur(sListeErreurs)
      	FIN	
     type : 458752
   -
     name : RécupèreInfoGraveur
     procedure_id : 1751867484447974737
     type_code : 14
     code : |1-
      // Résumé : Récupère les informations sur le graveur
      // Syntaxe :
      //RécupèreInfoGraveur ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      Procédure RécupèreInfoGraveur()
      
      	sListeGraveurs 	est une chaine
      	sGraveur	  	est une chaine
      	sIdentifiant	est une chaine
      	sNomGraveur		est une chaine
      	
      	LIB_TITRE = "Initialisation..."
      	
      	// Récupère la liste des graveurs installés
      	sListeGraveurs = GraveurListe(vrai)
      	
      	// Parcours la liste des graveurs
      	pour chaque chaine sGraveur de sListeGraveurs separee par rc
      		
      		// Extrait l'identifiant du graveur
      		sIdentifiant = extraitchaine(sGraveur, 1)
      		
      		// Extrait le nom du graveur
      		sNomGraveur = extraitchaine(sGraveur, 2) + [" "] + ExtraitChaîne(sGraveur, 3)
      		
      		listeajoute(COMBO_GRAVEUR, glien(sIdentifiant) + sNomGraveur)		
      	FIN
      	
      	LIB_TITRE = "Sélection du graveur..."
      	
      	// Si il n'y a qu'un seul graveur, on le sélectionne automatiquement
      	si COMBO_GRAVEUR..occurrence = 1 ALORS
      		
      		// Sélectionne le premier graveur
      		ListeSelectPlus(COMBO_GRAVEUR, 1)
      		GraveurSélectionne(COMBO_GRAVEUR)
      		
      		// Automatise la sélection du graveur
      		ExécuteTraitement(BTN_VALIDER_GRAVEUR, trtclic)
      	
      	// Si il n'y a aucun graveur
      	sinon si COMBO_GRAVEUR..Occurrence = 0 alors
      		
      		// L'éjection n'est possible
      		BTN_EJECTER_MEDIA..Etat = Grisé
      		BTN_LISTE_FICHIERS..Etat = Grisé
      		BTN_NOM_MEDIA..Etat = Grisé
      		BTN_AJOUT_FICHIER..Etat = Grisé
      		
      		// Pas de remplissage
      		Evénement_JaugeRemplissage(0)
      		
      		// Aucune icône spécifique
      		IMG_TYPE_MEDIA = ""
      		IMG_TYPE_MEDIA..CurseurSouris = curFlèche
      		
      		// Pas de graveur
      		gbGraveurPrésent = Faux
      		
      		// Affiche une erreur
      		Erreur("Aucun graveur n'est installé sur cet ordinateur")				
      		retour
      		
      	// On doit afficher la liste des graveurs
      	sinon
      		
      		// Affiche le premier plan de la fenêtre qui permet de sélectionner le graveur à utiliser
      		mafenetre..plan = 2
      		ComboOuvre(COMBO_GRAVEUR)
      	FIN
      	
      	// Active la détection d'insertion de média dans les lecteurs
      	fDétecteStockageAmovible(Evénement_DétecteInsertionCD)
      	
      FIN :
      	LIB_TITRE = "Prêt"
     type : 458752
   -
     name : Evénement_DétecteInsertionCD
     procedure_id : 1751888817554078771
     type_code : 14
     code : |1-
      // Résumé : Gère l'événement d'insertion d'un CD
      // Syntaxe :
      //Evénement_DétecteInsertionCD (<sLettreLecteur> est chaîne, <nTypeAction> est entier)
      //
      // Paramètres :
      //	sLettreLecteur (chaîne) : <indiquez ici le rôle de sLettreLecteur>
      //	nTypeAction (entier) : <indiquez ici le rôle de nTypeAction>
      // Valeur de retour :
      // 	Aucune
      //
      Procédure Evénement_DétecteInsertionCD(local sLettreLecteur est une chaine, nTypeAction est un entier)
      
      	
      
      	// Ne traite l'événement d'insertion que s'il s'agit d'un lecteur de CD-ROM ou un graveur
      	si fDisqueInfo(sLettreLecteur, fdTypeDisque) <> fdCDRom alors retour
      	
      	// Si le lecteur concerné n'est pas celui du graveur, on ne traite pas l'événement
      	si pas gsGraveurChemin [= sLettreLecteur alors retour
      
      	// Gère l'événement d'insertion
      	// (On utilise ici un PostMessage pour exécuter la procédure InsertionCD, en effet, l'événement d'insertion étant reçu dans un traitement parallèle à l'application, 
      	// la récupération de l'état du graveur n'est pas possible. Il est donc nécessaire d'information le traitement principal de l'application grâce à PostMessage)
      	postMessage(handle(), "INSERTION_CDROM", nTypeAction, 0)
     type : 458752
   -
     name : RécupèreEtatGraveur
     internal_properties : CAAAAAgAAAAOP6HZ57I8cW5Cz3iPCcsuKHHWVco32by1mxqUEAFQ97ZDsbF3u3HkqfbruRiw1ums+4VguiQN7HvruMJrCrUobQtv2LxSO86LxEr1J7rDyK/s58ohtEMYGnShAj8SU6KHxP5FZmt6ob5CKrHV
     procedure_id : 1751895397444066732
     type_code : 14
     code : |1-
      // Résumé : Tente de récupérer l'état du graveur
      // Syntaxe :
      //[ <Résultat> = ] RécupèreEtatGraveur ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	entier : Etat du graveur
      //
      Procédure RécupèreEtatGraveur()
      
      	nEtatGraveur est un entier
      	nNbEssais	 est un entier
      
      	// Sélectionne le graveur en cours
      	GraveurSélectionne(COMBO_GRAVEUR)
      
      	// Récupère l'état du graveur
      	nEtatGraveur = GraveurEtat()
      	
      	sablier(vrai)
      	IMG_SABLIER..Animation = vrai
      	
      	// En cas d'erreur temporaire
      	tantque nEtatGraveur = -1 _et_ nNbEssais <= 3
      
      		// Augmente le nombre d'essais		
      		nNbEssais ++
      		
      		// Laisse le système effectuer ses propres opérations
      		Multitâche(-100)
      		
      		// Récupère l'état du graveur
      		nEtatGraveur = GraveurEtat()
      	FIN
      		
      	IMG_SABLIER..Animation = faux
      	IMG_SABLIER = IMG_SABLIER
      	
      	Sablier(faux)
      		
      renvoyer nEtatGraveur
     type : 458752
   -
     name : Evénement_InsertionCD
     procedure_id : 1751902007398766980
     type_code : 14
     code : |1-
      // Résumé : Gère l'événement d'insertion
      // Syntaxe :
      //[ <Résultat> = ] Evénement_InsertionCD ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	booléen : Vrai
      //
      Procédure Evénement_InsertionCD()
      
      	nTypeAction est un entier = _eve.wparam
      
      	// On vient d'insérer le média dans le lecteur
      	SI nTypeAction = StockageAmovibleInséré ALORS
      		
      		// Récupère les informations sur le média inséré
      		RécupèreInfoMedia()		
      		
      		// Revient au premier plan
      		MaFenêtre..Plan = 1		
      		
      		// Fin de l'attente
      		IMG_SABLIER..Animation = Faux		
      		
      	SINON
      		
      		// Aucune icône spécifique
      		IMG_TYPE_MEDIA = ""
      		IMG_TYPE_MEDIA..CurseurSouris = curFlèche
      		
      		// Modifie l'image de fond de la fenêtre
      		MaFenêtre..ImageFond = "\images\graveur-vide.png"		
      		
      		// Aucun média présent dans le lecteur
      		gbMédiaPrésent = Faux
      	FIN
      	
      RENVOYER vrai
     type : 458752
