     internal_properties : CAAAAAgAAAB6y3Pc80CyMaKgRUx7QSfgcJsGhyJ5ncAN3RQAfEnoPUqhUZurNfWgPWxtPfQ4xk8oCSVCqura7VgOU7dSFTC7lhxw3dOhTNu0PwX6biVeY7BNCEOsB5DRw6tYPw5lBs2YlePCzeI1aNGcXkWh
     procedure_id : 1752153885753077918
     type_code : 14
     code : |1-
      // Résumé : Gère l'événement de gravure
      // Syntaxe :
      //Evénement_GravureEncours (<nEvénement> est entier [, <sMessageEvénement> [, ...]])
      //
      // Paramètres :
      //	nEvénement (entier) : Evénement
      //	sMessageEvénement (valeur par défaut="") : Message associé à l'événement
      // 	<*> : Autres données de l'événement
      // Valeur de retour :
      // 	Aucune
      //
      Procédure Evénement_GravureEncours(local nEvénement est un entier, local sMessageEvénement = "", *)
      
      	LIB_TITRE = "Gravure en cours..."
      
      	// Fin de gravure
      	si nEvénement dans (graveurNotificationAbandon, graveurNotificationErreur, graveurNotificationFinGravure) ALORS
      		
      		// Réactive les champs
      		IMG_ANIMDVD..Visible 	= faux
      		IMG_ANIMDVD..Animation 	= faux
      		IMG_ANIMDVD				= IMG_ANIMDVD
      		BTN_GRAVURE..Etat 		= actif
      		BTN_EJECTER_MEDIA..Etat = actif
      		BTN_LISTE_FICHIERS..Etat= actif
      		BTN_NOM_MEDIA..Etat		= actif
      		BTN_AJOUT_FICHIER..Etat	= actif
      		IMG_TYPE_MEDIA..Etat 	= actif
      		LIB_TITRE				= "Prêt"
      		LIB_INFO 				= ""
      		JAUGE_GRAVURE..visible	= faux
      		
      		selon nEvénement 
      			
      			// Abandon
      			cas graveurNotificationAbandon
      				Avertissement("La gravure a été abandonnée")
      			
      			// Erreur	
      			cas graveurNotificationErreur
      				erreur("Une erreur s'est produite pendant la gravure.", sMessageEvénement)
      				
      			// Fin de la gravure
      			cas graveurNotificationFinGravure
      				info("La gravure est terminée")
      			
      		FIN
      		
      		// Relance la détection
      		PostMessage(handle(), "FIN_GRAVURE", nEvénement, 0)
      		
      	sinon
      		
      		// En fonction de l'événement
      		selon nEvénement
      			
      			// Le graveur prépare la gravure
      			cas graveurNotificationPrepareGravure
      				LIB_INFO = "Préparation en cours..."
      				
      				// Affiche la jauge de progression
      				JAUGE_GRAVURE..Visible	= vrai
      				
      			// Le graveur termine la gravure
      			CAS graveurNotificationFermetureCD
      				LIB_INFO = "Fin de la gravure..."
      				
      				// Cache la jauge de progression
      				JAUGE_GRAVURE..Visible	= faux
      				
      			// Le graveur est en train d'écrire
      			cas graveurNotificationGravurePiste, graveurNotificationGravureBloc
      				lib_info = "Ecriture en cours..."
      				
      				// Complète jauge de progression
      				JAUGE_GRAVURE..valeur 	= MesParamètres[4]	// Bloc/Piste en cours
      				JAUGE_GRAVURE..BorneMax = MesParamètres[5]	// Blocs/Pistes au total
      				fenrepeint()
      				
      			// Evénement inconnu
      			AUTRE CAS
      				lib_info = ""
      				
      		FIN	
      		
      	FIN
     type : 458752
