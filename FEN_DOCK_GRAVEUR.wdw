  -
    name : BTN_LISTE_FICHIERS
    identifier : 0x2850f1f45e9bfaaa
    internal_properties : 
    properties :
     x : 59
     y : 178
     width : 24
     height : 24
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1-
         
         // La liste est vide
         si gsListeFichiers = "" ALORS
         	
         	info("Il n'y a pas de fichier à graver")
         	retour	
         FIN
         
         sablier(vrai)
         ArbreSupprimeTout(ARBRE_LISTEFICHIERS)
         
         sFichier est une chaine
         sType	 est une chaine
         sVolume	 est une chaine = GraveurNomVolume()
         sListe	 est une chaine
         sElément est une chaine
         sRep	 est une chaine
         
         ArbreAjoute(ARBRE_LISTEFICHIERS, sVolume, "\images\cd.gif", "\images\cd.gif")
         
         // Pour chaque fichier de la liste
         pour chaque chaine sFichier de gsListeFichiers separee par rc
         	
         	// Extrait le type (fichier ou répertoire)
         	sType = extraitchaine(sFichier, 1, "\")
         	
         	selon sType
         		
         		// Répertoire
         		cas "REP"
         			
         			// Extrait le nom du répertoire
         			sRep 	 = extraitchaine(ExtraitChaîne(sFichier, 1, tab), 2, "\")
         			sFichier = ExtraitChaîne(sFichier, 2, tab)
         		
         			// Récupère la liste des fichiers/répertoires du répertoire
         			sListe = fListeFichier(sFichier + ["\"] + "*.*", frRécursif)
         			
         			pour chaque chaine sElément de sListe separee par rc
         				
         				// Extrait le nom de l'élément
         				sElément = extraitchaine(sElément, 2, sFichier + ["\"])
         				
         				// Ajoute le fichier dans l'arbre
         				ArbreAjoute(ARBRE_LISTEFICHIERS, sVolume + TAB + sRep + tab + remplace(sElément, "\", tab), "\images\fichier.gif", "\images\fichier.gif")
         				
         			FIN
         		
         		// Fichier
         		cas "FIC"
         						
         			// Extrait le nom du fichier
         			sFichier = ExtraitChaîne(sFichier, 2, "\")
         			
         			// Ajoute le fichier dans l'arbre
         			arbreajoute(ARBRE_LISTEFICHIERS, sVolume + tab + sFichier, "\images\fichier.gif", "\images\fichier.gif")
         			
         	FIN
         	
         FIN
         
         // Déroule la racine
         ArbreDéroule(ARBRE_LISTEFICHIERS, sVolume)
         
         sablier(faux)
         
         AnimationPrépare(MaFenêtre)
         
         // Affiche le plan de la liste des fichiers
         mafenetre..plan = 5
         MaFenêtre..Hauteur = 400
         
         // Remonte la fenêtre si elle ne peut pas être affichée entièrement
         si mafenetre..y + MaFenêtre..Hauteur > sysyres - 40 alors
         	MaFenêtre..Y = SysYRes() - 40 - MaFenêtre..Hauteur
         FIN
         
         animationjoue(1)
        type : 18
      -
        type : 161
        enabled : false
